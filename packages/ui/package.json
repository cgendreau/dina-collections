  "proxy": {
    "/auth": {
      "target": "https://alpha-cm.dina-web.net",
      "xfwd": true,
      "changeOrigin": true
    },
    "/api": {
      "target": "http://127.0.0.1:4444",
      "xfwd": true,
      "changeOrigin": true
    }
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "babel-plugin-react-css-modules": "^3.3.3",
    "common": "file:../common",
    "debug": "^3.1.0",
    "dns": "file:./src/mock/dns",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "json-schema-faker": "^0.4.4",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "markdown-it": "^8.4.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^0.5.2",
    "prettier": "1.8.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-json-inspector": "^7.1.1",
    "react-list": "^0.8.8",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.17",
    "react-sortable-tree": "^2.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-module-local-storage": "^1.1.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.77.2",
    "watch-glob": "^0.1.3",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "yarn build-css && yarn markdown:build && REACT_APP_ENABLE_MOCK=false react-scripts build",
    "build-css": "node-less-chokidar src/ -o src/",
    "build-snippets": "node ./src/scripts/snippets/index.js",
    "build:mock": "yarn build-css && yarn markdown:build && REACT_APP_ENABLE_MOCK=true react-scripts build",
    "build:storybook": "yarn update-storybook-imports && build-storybook -c .storybook -o build-storybook",
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "eject": "react-scripts eject",
    "ensure-no-diff": "git diff --exit-code",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "markdown:build": "node src/scripts/markdown/buildRenderedMarkdownFiles.js",
    "markdown:watch": "node src/scripts/markdown/watchMarkdownFiles.js",
    "mount:admin": "MOUNT_APP=admin node ./src/scripts/app/mountApp.js",
    "mount:collectionsUi": "MOUNT_APP=collectionsUi node ./src/scripts/app/mountApp.js",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx}'",
    "start": "NODE_PATH=./src/ npm-run-all -p watch-css markdown:watch start-js",
    "start-js": "react-scripts start",
    "start:mock": "REACT_APP_ENABLE_MOCK=true yarn start",
    "storybook": "yarn update-storybook-imports && start-storybook -p 9009 -s public",
    "test": "yarn update-storybook-imports && yarn lint:js && react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "test:unit:fast": "REACT_APP_TEST_UI=false react-scripts test --env=jsdom --watch",
    "test:unit:watch": "react-scripts test --env=jsdom --watch",
    "update-git-dependencies": "yarn upgrade models && yarn upgrade semantic-ui",
    "update-storybook-imports": "node ./src/scripts/storybook/createStorybookImports",
    "watch-css": "yarn build-css && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-info": "3.2.12",
    "@storybook/addon-knobs": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/addon-storyshots": "3.2.12",
    "@storybook/react": "3.2.12",
    "babel-eslint": "^8.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "deep-freeze": "^0.0.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest-localstorage-mock": "^2.0.1",
    "lodash": "^4.3.2",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.2.0",
    "source-map-explorer": "^1.5.0",
    "storybook-readme": "3.0.6"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.3.2"
  }
}